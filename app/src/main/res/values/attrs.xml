<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundImageView">
        <attr name="type" format="enum">
            <enum name="circle" value="1"/>
            <enum name="round" value="2"/>
        </attr>
        <attr name="radius" format="dimension"/>
    </declare-styleable>


    <declare-styleable name="RoundProgress">
        <!--背景圆形颜色 -->
        <attr name="bgColor" format="color"/>
        <!--进度条颜色（未达到的进度部分） -->
        <attr name="roundColor" format="color"/>
        <!--进度条及背景圆形的宽度 -->
        <attr name="roundWidth" format="dimension"/>
        <!--中心字体的大小 -->
        <attr name="roundTextSize" format="dimension"/>
        <!--中心字体的颜色以及进度条颜色（已达到进度部分） -->
        <attr name="roundTextColor" format="color"/>
        <!--当前进度-->
        <attr name="curProgress" format="integer"/>
        <!--提示的文字-->
        <attr name="tipsText" format="string"/>
    </declare-styleable>
</resources>